<body>
    <!-- navbar sections starts  -->
    <header class="header">

        <div class="logo_container">
            <a href="#"><img src="images/case.png" alt="Logo"></a>
        </div>



    </header>
    <!-- navbar sections starts  -->

   
    <!-- home section stars  -->

    <section class="home" id="home">
        <div class="content">
            <h1>Login</h1>
            <p>Silahkan login!</p>

            <form  method="POST" action="/login">
                <!-- Email input -->
                <div class="form-outline mb-4">
                  <input type="email" id="form1Example13" class="form-control form-control-lg" autocapitalize="false" name="temail"/>
                  <label class="form-label"   for="form1Example13">Email</label>
                </div>
      
                <!-- Password input -->
                <div class="form-outline mb-4">
                  <input type="password" id="form1Example23" class="form-control form-control-lg" name="tpassword" />
                  <label class="form-label"   for="form1Example23">Password</label>
                </div>
      
                <div class="d-flex justify-content-around align-items-center mb-4">
                  <!-- Checkbox -->

                </div>
      
                <!-- Submit button -->
                <button type="submit" class="btn btn-primary btn-lg btn-block">Sign in</button>
      
              </form>

            

        </div>


        <div class="image">
            <img src="images/login.jpg" alt="illustration-hero" class="resized-image">
        </div>
    </section>

    <!-- home section ends -->



    


    <!-- footer section starts  -->

   <section class="footer" id="footer">
